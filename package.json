{
  "name": "transformerai",
  "displayName": "transformerAI",
  "description": "Обучение модели на базе своего коде",
  "version": "0.0.4",
  "publisher": "Rus7Iv",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onCommand:transformerai.generateCode"
  ],
  "contributes": {
    "configuration": {
      "title": "transformerAI",
      "properties": {
        "transformerAI.apiKey": {
          "type": "string",
          "default": "",
          "description": "API key for AI21 Studio"
        }
      }
    },
    "commands": [
      {
        "command": "transformerai.generateCode",
        "title": "Сгенерировать код по комментарию"
      },
      {
        "command": "transformerai.setApiKey",
        "title": "Установить ключ API для AI21 Studio"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "openai": "^4.79.1"
  }
}